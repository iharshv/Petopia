{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.{html,css,js,png,jpg,jpeg,svg,gif,ico}",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}